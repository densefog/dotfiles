{
  "diagnostic.enable": true,
  "diagnostic.displayByAle": true,
  "coc": {
     "preferences" : {
       "formatOnSaveFiletypes": ["css", "markdown", "javascript", "graphql", "html", "yaml",  "json", "python"],
       "currentFunctionSymbolAutoUpdate": true,
       "messageLevel": "error"
     }
  },
  "codeLens.enable": true,
  "languageserver": {
    "elixirLS": {
      "command": "~/elixir-ls/rel/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    }
  }
}
