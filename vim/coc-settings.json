{
  "diagnostic.enable": true,
  "diagnostic.displayByAle": false,
  "coc": {
    "preferences": {
      "formatOnSaveFiletypes": [
        "css",
        "markdown",
        "javascript",
        "graphql",
        "html",
        "yaml",
        "json",
        "python"
      ],
      "currentFunctionSymbolAutoUpdate": true,
      "messageLevel": "error"
    }
  },
  "codeLens.enable": true,
  "elixir.pathToElixirLS": "~/elixir-ls/rel/language_server.sh",
  "elixirLS.suggestSpecs": false,
  "languageserverx": {
    "elixirLS": {
      "command": "~/elixir-ls/release/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    }
  }
}
